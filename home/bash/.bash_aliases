alias install='sudo apt-get install -y'
alias remove='sudo apt-get remove -y'
alias scripts='pushd ~/scripts'
alias ssh-keys='pushd ~/.ssh'
alias edit-alias='nano ~/.bash_aliases'
alias edit-bashrc='nano ~/.bashrc'
alias source-bash='source ~/.bashrc'
alias update='sudo bash ~/.bin/update'
alias projects='pushd ~/Documents/Projects'
alias config='pushd ~/Documents/Config'
alias apt-search='sudo apt-cache search'
